{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]",
          "video[playsinline]"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-flex",
          "-ms-touch-action",
          "-webkit-text-size-adjust",
          "overscroll-behavior",
          "scrollbar-width",
          "overscroll-behavior: none",
          "scrollbar-width: none",
          "scrollbar-color",
          "-webkit-overflow-scrolling",
          "overflow: clip",
          "-webkit-user-modify",
          "-webkit-mask-box-image"
        ]
      }
    ]
  }
}